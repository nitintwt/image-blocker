{
  "manifest_version": 3 ,
  "name": "Image Blocker" ,
  "description" : "An extension to block and unblock images of a web page" ,
  "version": "1.0",
  "background": { "service_worker" : "background.js"},
  "content_scripts" : [{"matches": ["<all_urls>"] , "js":["content.js"]}],
  "permissions": ["activeTab","scripting" , "storage"],
  "action":{  
    "default_icons": {
    "16": "images/image-blocker16.png",
    "32": "images/image-blocker16.png",
    "48": "images/image-blocker16.png",
    "128": "images/image-blocker16.png"
  }},
  "icons": {
    "16": "images/image-blocker16.png",
    "32": "images/image-blocker16.png",
    "48": "images/image-blocker16.png",
    "128": "images/image-blocker16.png"
  }
}
